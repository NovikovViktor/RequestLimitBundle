{
  "name" : "nv/request-limit-bundle",
  "type" : "symfony-bundle",
  "description" : "This bundle provides a light way to restrict user access to some action for a specific time frame",
  "keywords" : [
    "POST",
    "GET",
    "HTTP verbs",
    "Symfony",
    "Request",
    "Restrict",
    "Limitations"
  ],
  "homepage": "https://github.com/NovikovViktor/RequestLimitBundle",
  "license": "MIT",
  "authors" : [
    {
      "name" : "Viktor Novikov"
    }
  ],
  "require" : {
    "php": "^7",
    "ext-memcached": "*",
    "ext-pdo": "*",
    "symfony/symfony": ">=3.0",
    "doctrine/common": "~2.4"
  },
  "require-dev": {
    "phpspec/phpspec": "*",
    "friendsofphp/php-cs-fixer": "*",
    "doctrine/dbal": "~2.4",
    "doctrine/orm": "~2.4,>=2.4.5",
    "doctrine/doctrine-bundle": "~1.4",
    "phpunit/phpunit": "4.*.*"
  },
  "autoload" : {
    "psr-4" : { "NV\\RequestLimitBundle\\" : "" }
  }
}
